            app:layout_constraintEnd_toStartOf="@+id/guideline_vert_bor_bot"
            app:layout_constraintStart_toStartOf="@+id/guideline_vert_bor_top"
            app:layout_constraintTop_toTopOf="@+id/guideline_titulo" />

