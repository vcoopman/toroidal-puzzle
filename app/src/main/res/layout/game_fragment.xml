                android:src="@drawable/button_load"
                android:text="LOAD" />

            <ImageButton
                android:id="@+id/shuffleButton"
                style="@style/ButtonBarraGame"
                android:src="@drawable/button_shuffle"
                android:text="SHUFFLE" />
        </androidx.appcompat.widget.LinearLayoutCompat>

